<h1>Home#index</h1>
<p><%= debug session[:cart] %></p>

<% @categories.each do |category| %>
  <p>
    <%= category.categoryname %>
    <%= link_to category.categoryname, category_path(category) %>
  </p>
<% end %>

<h2> Checkout some of our featured products </h2>

<% @products.each do |product| %>
  <p>
    <%= link_to product.productname, product_path(product) %>
  </p>
<% end %>
